<script>$(document).ready(function () {
var jcrop_api;
$('#target').Jcrop({
onChange: showCoords,
onSelect: showCoords
}, function () {
jcrop_api = this;
var initial_width = Math.floor(600 -600 / 8);
var initial_height = Math.floor(337 -337 / 8);
var initial_x_offset = Math.floor(600 / 8);
var initial_y_offset = Math.floor(337 / 8);
jcrop_api.setSelect([initial_x_offset, initial_y_offset, initial_width, initial_height]);

});

$('#set-to').click(function (e) {
var r_x1 = Math.floor($('#x1').val());
var r_y1 = Math.floor($('#y1').val());
var r_x2 = r_x1 + Math.floor($('#w').val());
var r_y2 = r_y1 + Math.floor($('#h').val());
jcrop_api.setSelect([r_x1, r_y1, r_x2, r_y2]);
return false;
});

$('#square').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 1} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#as43').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 1.333333333} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#as169').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 1.777777778} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#as32').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 1.5} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#as21').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 2} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#as12').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 0.5} : {aspectRatio: 0});
jcrop_api.focus();
});

$('#golden').change(function (e) {
jcrop_api.setOptions(this.checked ? {aspectRatio: 1.618033988} : {aspectRatio: 0});
jcrop_api.focus();
});

});

function showCoords(c) {
$('#x1').val(Math.floor(c.x));
$('#y1').val(Math.floor(c.y));
$('#x2').val(Math.floor(c.x2));
$('#y2').val(Math.floor(c.y2));
$('#w').val(Math.floor(c.w));
$('#h').val(Math.floor(c.h));
}

</script>
